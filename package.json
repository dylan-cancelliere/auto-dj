{
  "name": "auto-dj-backend",
  "version": "1.0.0",
  "description": "Backend for AutoDJ using express and mongodb",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samharrell24/auto-dj-backend.git"
  },
  "author": "Joseph Vita, Samuel Harrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samharrell24/auto-dj-backend/issues"
  },
  "homepage": "https://github.com/samharrell24/auto-dj-backend#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "mongodb": "^3.5.3",
    "nodemon": "^2.0.2"
  }
}
